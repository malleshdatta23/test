# ---- BEGIN: Explicitly seed $$FEED_NAME (do not change logic) ----

feed_name = None

# Case 1: $$FEED_NAME under GLOBAL section
if session_param_json and "GLOBAL" in session_param_json:
    feed_name = session_param_json["GLOBAL"].get("$$FEED_NAME")

# Case 2: $$FEED_NAME directly under workflow
if not feed_name and session_param_json:
    feed_name = session_param_json.get("$$FEED_NAME")

if not feed_name:
    raise Exception("$$FEED_NAME is missing in parameter file aEDW_FIN_Disc_Daily.par")

Variable.set(
    id
    + ".wklt_NORM_100_500_O_FIN_DAILY_DISC"
    + ".s_m_100_CTL_ETL_FEED_PROCESS_START_NORM_FIN_DAILY_DISC"
    + ".$$FEED_NAME",
    feed_name
)

# ---- END: Explicitly seed $$FEED_NAME ----
